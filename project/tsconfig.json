{
	"compilerOptions": {
		"outDir": "./dist",
		"noImplicitAny": false,
		"sourceMap": true,
		"module": "CommonJS",
		"target": "es3",
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"lib": ["es2016", "DOM"],
		"baseUrl": ".",
		"paths": {
			"gulp-es3-typescript-library": [
				"node_modules/gulp-es3-typescript-library/dist"
			]
		}
	},
	"include": ["src"]
}
